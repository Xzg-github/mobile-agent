<view class="content"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="mainBody"><view class="item center"><view class="item-mane"><text class="tips">*</text>服务类型</view><view class="picker-box"><picker class="picker" mode="multiSelector" value="{{multiIndex}}" range-key="name" range="{{picketData}}" data-event-opts="{{[['columnchange',[['picketChange',['$event']]]]]}}" bindcolumnchange="__e"><view>{{''+(picketData[0][multiIndex[0]].name||'请选择')+"\n\t\t\t\t\t\t\t"+(picketData[1][multiIndex[1]].name||'')+"\n\t\t\t\t\t\t\t"+(picketData[2][multiIndex[2]].name||'')+''}}</view><text class="iconfont down"></text></picker></view></view><view><view class="item center"><text class="item-mane"><text class="tips">*</text>服务结果</text><view style="display:flex;"><view data-event-opts="{{[['tap',[['changeYes',['N']]]]]}}" style="display:flex;" bindtap="__e"><block wx:if="{{yes=='N'}}"><text class="iconfont ridao icn"></text></block><block wx:else><text class="iconfont ridao"></text></block><text>完成</text></view><view data-event-opts="{{[['tap',[['changeYes',['Y']]]]]}}" style="display:flex;margin-left:40rpx;" bindtap="__e"><block wx:if="{{yes=='Y'}}"><text class="iconfont ridao icn"></text></block><block wx:else><text class="iconfont ridao"></text></block><text>预约</text></view></view></view><block wx:if="{{yes=='Y'}}"><view class="section center" style="display:flex;"><view class="item-mane"><text class="tips">*</text>预约时间</view><view class="picker-box"><picker class="picker" mode="date" value="{{date}}" start="2015-09-01" end="2028-09-01" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view>{{date+" "+time}}</view><text class="iconfont down"></text></picker></view></view></block></view><view class="item"><text class="item-mane">服务小结</text><textarea name id cols="30" rows="10" data-event-opts="{{[['input',[['__set_model',['','serviceLog','$event',[]]],['bindTextAreaBlur',['$event']]]]]}}" value="{{serviceLog}}" bindinput="__e"></textarea></view></view><button class="btn" formType="submit">提交</button></form></view>