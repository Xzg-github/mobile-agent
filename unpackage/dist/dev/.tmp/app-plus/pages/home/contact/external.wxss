page {
  background: #f7f6f9;
}
.content {
  padding-bottom: 100rpx;
}
.main-wrap {
  margin-bottom: 30rpx;
  background: #fff;
  padding: 30rpx 0;
  font-size: 28rpx;
}
.main-wrap .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 34rpx;
  font-weight: bold;
  padding: 0 0 26rpx 40rpx;
  border-bottom: 1rpx solid #ccc;
}
.main-wrap .title .tit-text {
  width: 5rpx;
  border-radius: 4rpx;
  margin-right: 20rpx;
  background: #4a85d3;
  height: 30rpx;
}
.main-wrap .attributes {
  padding: 0 40rpx;
  color: #000;
}
.main-wrap .attributes .list {
  padding: 13rpx 0;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.main-wrap .attributes .list text {
  color: #666;
}

