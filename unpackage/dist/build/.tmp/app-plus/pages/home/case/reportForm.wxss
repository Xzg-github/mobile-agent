.content {
  padding-bottom: 90rpx;
}
.nav-box {
  margin: 60rpx 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background: #DBDBDB;
  border-radius: 8rpx;
}
.nav-box .btn {
  width: 50%;
  text-align: center;
  border-radius: 8rpx;
  margin: 12rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
}
.nav-box .active {
  background: #4a85d3;
  color: #fff;
}
.form-wrap {
  margin: 30rpx 40rpx 80rpx 30rpx;
  font-size: 28rpx;
}
.form-wrap .regForm-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 34rpx;
}
.form-wrap .regForm-item checkbox-group {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-flow: row wrap;
          flex-flow: row wrap;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.form-wrap .regForm-item .regForm-item-tit {
  font-size: 26rpx;
  width: 180rpx;
  padding-right: 40rpx;
  text-align: right;
}
.form-wrap .regForm-item .regForm-item-tit .tips {
  color: #f5222d;
  padding-right: 8rpx;
}
.form-wrap .regForm-item .list-cell {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 50%;
}
.form-wrap .regForm-item .list-cell checkbox {
  -webkit-transform: scale(0.6, 0.6);
          transform: scale(0.6, 0.6);
}
.form-wrap .regForm-item .ipt {
  display: block;
  padding: 0 10rpx;
  border: 1rpx solid #ccc;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 60rpx;
  line-height: 60rpx;
  position: relative;
}
.form-wrap .regForm-item .ipt .set {
  width: 100%;
  height: 60rpx;
}
.form-wrap .regForm-item .ipt .fr {
  position: absolute;
  top: 6rpx;
  right: 16rpx;
}
.form-wrap .regForm-item .textarea {
  padding: 10rpx;
  height: 70rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  border: 1rpx solid #ccc;
}
.form-wrap .regForm-item .data {
  width: 240rpx;
}
.form-wrap .regForm-item .time {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.form-wrap .regForm-item .time-text {
  margin-left: 10rpx;
}
.form-wrap .regForm-item .default {
  color: #808080;
}
.form-wrap .regForm-item .select {
  color: #333;
}
.submit {
  margin: 90rpx 30rpx;
  padding: 0;
  background: #4a85d3;
  color: #fff;
}
button:after {
  border: none;
}

