<view><view class="customer-info"><block wx:if="{{boot}}"><view class="questionnaire"><view class="head"><view class="head-item"><view class="item-name">问卷名称:</view><text>{{questionData.examName}}</text></view><view class="head-item"><view class="item-name">问卷描述:</view><text>{{questionData.remark}}</text></view><view class="head-item"><view class="item-name">引导语:</view><block wx:if="{{questionData.guide!=null}}"><text>{{questionData.guide}}</text></block></view></view><view><text class="subject">{{questionObj.subjectTitle}}</text><view class="content"><view class="left"><block wx:for="{{questionObj.examItemList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeKey',[index,'$0','$1'],[[['questionObj.examItemList','',index,'id']],[['questionObj.examItemList','',index,'nextSubjectId']]]]]]]}}" bindtap="__e"><block wx:if="{{sclet==index}}"><text class="iconfont leftIcon" style="color:#007AFF;"></text></block><block wx:else><text class="iconfont leftIcon"></text></block><text>{{item.itemName}}</text></view></block></view><block wx:if="{{questionObj.subjectSpeech!=null}}"><text class="right">{{questionObj.subjectSpeech}}</text></block></view><view class="btn-box"><block wx:if="{{cur_id!=0}}"><view data-event-opts="{{[['tap',[['goPrev',['$event']]]]]}}" class="go" bindtap="__e">上一题</view></block><block wx:if="{{questionObj.subjectType==1}}"><view data-event-opts="{{[['tap',[['goNext',['btn']]]]]}}" class="go" bindtap="__e">下一题</view></block></view></view></view></block><block wx:else><view style="text-align:center;padding:20rpx 0;">请选择号码进行外呼</view></block></view></view>